<?xml version='1.0' encoding='utf-8'?>
<widget id="uk.ac.edina.mobile.mbtilesmap" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MBTiles Map</name>
    <description>
        An experimental application for testing mbtiles served via http
    </description>
    <author email="ruben.gamez@ed.ac.uk" href="http://rgamez.github.io">
        EDINA Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <plugin name="https://github.com/rgamez/cordova-plugin-http-mbtiles" version="0.1.0" />
    <access origin="http://*.tile.osm.org/*" />
</widget>
